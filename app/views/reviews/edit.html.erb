<div class="col-md6 col-md-offset-3">
  <div class="edit-event-form">
<h1>Edit Review</h1>

<%= simple_form_for([@event, @review]) do |f| %>
<div id="ratting-form">
<label>Beri Kami Rating</label>
</div>
<%= f.input :comment %>
<%= f.button :submit, :class => "btn-custom2" %>
<% end %>
</div>
</div>

<script>
  $('#ratting-form').raty({
    path: '/assets',
    scoreName: 'review[ratting]'
  });
</script>
